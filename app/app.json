{
  "expo": {
    "name": "LynxAI",
    "slug": "lynx",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.ACCESS_MEDIA_LOCATION"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": "lynxai",
    "newArchEnabled": true,
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "Die App benötigt Zugriff auf Ihre Fotos, um Dokumente zu scannen.",
          "cameraPermission": "Die App benötigt Zugriff auf Ihre Kamera, um Dokumente zu scannen.",
          "microphonePermission": "Die App benötigt Zugriff auf Ihr Mikrofon."
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "Die App benötigt Zugriff auf Ihre Fotos, um Dokumente zu speichern.",
          "savePhotosPermission": "Die App benötigt Zugriff, um Dokumente in Ihrer Mediathek zu speichern.",
          "isAccessMediaLocationEnabled": true
        }
      ],
      "expo-localization"
    ],
    "extra": {
      "eas": {
        "projectId": "c63e2c87-c44d-4502-abbd-06d2b2af2aeb"
      }
    },
    "owner": "weblabsio"
  }
}
